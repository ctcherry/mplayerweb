<!doctype html>
<html>
<head>
<title>Media Control</title>
<link href="http://fonts.googleapis.com/css?family=Open+Sans:400,700" rel="stylesheet" type="text/css">
<link rel="stylesheet" type="text/css" href="/style.css" />
</head>
<body class="movies">
<div id="page">

<nav class="main">
<ul class="left">
<li><a href="/tv">TV</a></li>
<li><a href="/movies">Movies</a></li>
<li><a href="/music">Music</a></li>
</ul>
<ul class="right">
<li id="status"></li>
<li><a target="hframe" href="/stop" class="stop">Stop</a></li>
<li><a href="/rescan" class="rescan">Rescan</a></li>
</ul>
</nav>

<nav class="side">

</nav>

<div id="content">
	<h1>Music</h1>

	<ul class="movie_list">
	<% _.each(difm_stations, function(st) { %>
		<li><a target="hframe" href="/play/list?<%= st[1] %>"><%= st[0] %></a></li>
	<% }); %>
	</ul>


</div>

</div>
<input type="text" id="terminal"></input>
<iframe name="hframe"></iframe>
<script src="//cdnjs.cloudflare.com/ajax/libs/zepto/1.0rc1/zepto.min.js" type="text/javascript"></script>
<script type="text/javascript" src="/app.js"></script>
</body>
</html>